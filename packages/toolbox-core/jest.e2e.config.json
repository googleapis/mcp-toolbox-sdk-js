{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
        "<rootDir>/test/e2e/*.e2e.ts"
    ],
     "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json"
        }
      ]
    },
    "globalSetup": "<rootDir>/test/e2e/jest.globalSetup.ts",
    "globalTeardown": "<rootDir>/test/e2e/jest.globalTeardown.ts",
    "testTimeout": 60000,
    "collectCoverage": false
}